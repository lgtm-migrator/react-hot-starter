{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    { "language": "html", "autoFix": true }
  ],
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "search.usePCRE2": true,
  "cSpell.words": [
    "Emptiable",
    "HOCS",
    "Signin",
    "Signout",
    "crossorigin",
    "immer",
    "linebreak",
    "multilines",
    "paren",
    "parens",
    "paypal",
    "storysource",
    "xstate"
  ]
}
